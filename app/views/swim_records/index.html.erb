<script src="http://code.angularjs.org/1.2.8/angular-animate.min.js"></script>
<div data-ng-app='comsatrack'>
  <div data-ng-controller='SwimRecordsCtrl'>
    <% provide(:title, 'Check Out') %>
    <alert ng-repeat="alert in alerts" type="alert.type" close="closeAlert($index)">{{alert.msg}}</alert>
    <h1 class='hidden-xs'>Swimmer Check Out</h1>
    <center><h4 class='visible-xs'>Swimmer Check Out</h4></center>
    <div class='row'>
      <div class='col-md-3'>
        <div class="input-group">
          <input class="form-control" ng-model="search"
                 placeholder="Search Last Name"/>
          <span class="input-group-addon">
            <span class="glyphicon glyphicon-search"></span>
          </span>
        </div>
      </div>
      <div class='col-md-7'></div>
      <div class='col-md-2'>
    </div>
    <div class='hidden-xs'>
      <br><br><br>
    </div>
    <%= render 'index' %>
  </div>
</div>


